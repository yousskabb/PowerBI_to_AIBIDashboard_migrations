{
  "datasets": [
    {
      "name": "ds01main",
      "displayName": "orders-data",
      "queryLines": [
        "SELECT ",
        "  Region, ",
        "  Category, ",
        "  `Sub-Category` AS Sub_Category, ",
        "  Segment, ",
        "  City, ",
        "  SUM(Profit) AS Profit, ",
        "  SUM(Sales) AS Sales, ",
        "  SUM(Quantity) AS Quantity ",
        "FROM <your-catalog>.dashboard_sample___superstore.orders ",
        "GROUP BY Region, Category, `Sub-Category`, Segment, City"
      ]
    }
  ],
  "pages": [
    {
      "name": "pg01main",
      "displayName": "Overview",
      "layout": [
        {
          "widget": {
            "name": "w1titleb",
            "textbox_spec": "# Sample Superstore Report"
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 1 }
        },
        {
          "widget": {
            "name": "w9filter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds01main",
                  "fields": [
                    { "name": "City", "expression": "`City`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "filter-single-select",
              "encodings": {
                "fields": [
                  {
                    "fieldName": "City",
                    "displayName": "City",
                    "queryName": "main_query"
                  }
                ]
              },
              "frame": { "showTitle": true, "title": "City" }
            }
          },
          "position": { "x": 0, "y": 1, "width": 1, "height": 1 }
        },
        {
          "widget": {
            "name": "w2profit",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds01main",
                  "fields": [
                    { "name": "Profit", "expression": "SUM(`Profit`)" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": { "fieldName": "Profit", "displayName": "Total Profit" }
              },
              "frame": { "showTitle": true, "title": "Total Profit" }
            }
          },
          "position": { "x": 1, "y": 1, "width": 2, "height": 1 }
        },
        {
          "widget": {
            "name": "w3quanti",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds01main",
                  "fields": [
                    { "name": "Quantity", "expression": "SUM(`Quantity`)" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": { "fieldName": "Quantity", "displayName": "Total Quantity" }
              },
              "frame": { "showTitle": true, "title": "Total Quantity" }
            }
          },
          "position": { "x": 3, "y": 1, "width": 2, "height": 1 }
        },
        {
          "widget": {
            "name": "w4saless",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds01main",
                  "fields": [
                    { "name": "Sales", "expression": "SUM(`Sales`)" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": { "fieldName": "Sales", "displayName": "Total Sales" }
